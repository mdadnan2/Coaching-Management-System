# Frontend Environment Variables Template
# Copy this file to .env.development or .env.production

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:5010
REACT_APP_API_VERSION=v1
REACT_APP_API_TIMEOUT=30000

# App Configuration
REACT_APP_NAME=Coaching Management System
REACT_APP_ENV=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=true

# Storage Keys (for localStorage/sessionStorage)
REACT_APP_STORAGE_PREFIX=coaching_app_
REACT_APP_TOKEN_KEY=auth_token
REACT_APP_REFRESH_TOKEN_KEY=refresh_token
